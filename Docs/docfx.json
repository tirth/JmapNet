{
    "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
    "metadata": [
        {
            "src": [
                {
                    "src": "../",
                    "files": [
                        "JmapNet/JmapNet.csproj",
                        "JmapNet.Client/JmapNet.Client.csproj",
                        "JmapNet.Dns/JmapNet.Dns.csproj"
                    ]
                }
            ],
            "dest": "api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "exclude": [
                    "_site/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "output": "_site",
        "template": [
            "default",
            "modern"
        ],
        "globalMetadata": {
            "_appName": "JmapNet",
            "_appTitle": "JmapNet",
            "_appFooter": "Copyright (c) 2025 Tirth Patel",
            "_disableContribution": true,
            "_enableSearch": true,
            "pdf": false
        }
    }
}
